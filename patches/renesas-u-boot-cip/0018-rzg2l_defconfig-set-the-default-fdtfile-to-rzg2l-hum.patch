From 45eac66c73a423fe93f6b28a1f892766e659cc81 Mon Sep 17 00:00:00 2001
From: Yazan Shhady <yazan.shhady@solid-run.com>
Date: Sun, 2 Jul 2023 12:16:31 +0300
Subject: [PATCH] rzg2l_defconfig - set the default fdtfile to
 rzg2l-hummingboard-ripple.dtb

---
 configs/rzg2l-solidrun_defconfig | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/configs/rzg2l-solidrun_defconfig b/configs/rzg2l-solidrun_defconfig
index 798b1429fb..18413dcea7 100644
--- a/configs/rzg2l-solidrun_defconfig
+++ b/configs/rzg2l-solidrun_defconfig
@@ -17,7 +17,7 @@ CONFIG_FIT=y
 CONFIG_USE_BOOTARGS=y
 CONFIG_BOOTARGS="root=/dev/mmcblk0p2 rw rootwait"
 CONFIG_SUPPORT_RAW_INITRD=y
-CONFIG_DEFAULT_FDT_FILE="rzg2l-solidrun.dtb"
+CONFIG_DEFAULT_FDT_FILE="rzg2l-hummingboard-ripple.dtb"
 CONFIG_VERSION_VARIABLE=y
 CONFIG_HUSH_PARSER=y
 CONFIG_CMD_BOOTZ=y
