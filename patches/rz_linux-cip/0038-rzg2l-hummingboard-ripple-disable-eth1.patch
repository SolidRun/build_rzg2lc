+++ b/arch/arm64/boot/dts/renesas/rzg2l-hummingboard-ripple.dts
@@ -186,3 +186,7 @@ &usb1_vbus {
 	gpio = <&pinctrl RZG2L_GPIO(42, 0) GPIO_OPEN_DRAIN>;
 	gpio-open-drain;
 };
+
+&eth1 {
+	status = "disabled";
+};
\ No newline at end of file
